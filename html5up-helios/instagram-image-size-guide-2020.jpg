/*
Theme Name: Tailwind - TheMotion Child Theme
Description: A <a href='http://codex.wordpress.org/Child_Themes'>Child Theme</a> for Tailwind.
Version: 1.0.1
Author: Steed Interactive
Author URI: https://steedokc.com
Template: themotion
*/

body,
button,
input,
select,
textarea {
  font-family: 'Open Sans', -apple-system, BlinkMacSystemFont, 'Segoe UI',
    Roboto, Helvetica, Arial, sans-serif, 'Apple Color Emoji', 'Segoe UI Emoji',
    'Segoe UI Symbol';
  font-size: 18px;
  line-height: 1.6;
  color: #555;
}

body {
  font-family: 'Open Sans', -apple-system, BlinkMacSystemFont, 'Segoe UI',
    Roboto, Helvetica, Arial, sans-serif, 'Apple Color Emoji', 'Segoe UI Emoji',
    'Segoe UI Symbol' !important;
}

p {
  margin: 0 0 30px;
}

.home-top-area {
  padding: 200px 0;
}

.home-icons-row {
  display: flex;
  padding-top: 20px;
  flex-wrap: wrap;
}

.home-icon {
  display: flex;
  flex-direction: column;
  flex-grow: 1;
  align-items: center;
  margin: 8px;
}

.home-icon > a > span {
  max-width: 200px;
  text-align: center;
  display: block;
  margin-top: 10px;
}

.home-icon > a > img {
  transform: translatey(0);
  transition-duration: 0.2s;
}

.home-icon > a:hover > img {
  transform: translatey(-5px);
}

/* Side Menu Icons */
.side-icons-row {
  display: flex;
  flex-wrap: wrap;
  margin: 0 -20px;
  justify-content: center;
}

.side-icon {
  display: flex;
  flex-direction: column;
  flex: 0 0 90px;
  align-items: center;
  margin: 0;
  padding-top: 10px;
}

.side-icon > a {
  text-align: center;
}

.side-icon > a > span {
  text-align: center;
  display: block;
  font-size: 0.9em;
}

.side-icon > a > img {
  transform: translatey(0);
  transition-duration: 0.2s;
}

.side-icon > a:hover > img {
  transform: translatey(-5px);
}

.recently-item .entry-content p,
.blog-post .entry-content p {
  color: #555;
  font-size: 16px;
  line-height: 1.8;
}

.recently-item h2.entry-title a,
.entry-title-blog a {
  font-weight: 800;
  line-height: 1.3;
  letter-spacing: 1.25px;
  color: #333;
}
.recently-item h2.entry-title a:hover,
.entry-title-blog a:hover {
  font-weight: 800;
  line-height: 1.3;
  letter-spacing: 1.25px;
  color: #005275;
}

/* Override Font */

