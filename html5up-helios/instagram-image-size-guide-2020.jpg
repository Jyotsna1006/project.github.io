.widget_origin_call-to-action .subtitle,
.card-title,
.attachment .entry-header-content .entry-title,
input[type='submit'].edd-submit,
#jp-relatedposts
  > .jp-relatedposts-items
  > .jp-relatedposts-post.jp-relatedposts-post-thumbs
  > h4.jp-relatedposts-post-title,
.widget_categories li a,
.widget_recent_entries .post-date {
  font-family: 'Open Sans', -apple-system, BlinkMacSystemFont, 'Segoe UI',
    Roboto, Helvetica, Arial, sans-serif, 'Apple Color Emoji', 'Segoe UI Emoji',
    'Segoe UI Symbol';
}

.read-more-button {
  display: block;
  float: right;
}

.more-link {
  font-size: 16px;
  letter-spacing: 1px;
  font-weight: 700;
}

.more-link:hover {
  border-color: #0793ca;
  color: #fff;
  background: #0793ca;
}

@media screen and (min-width: 480px) .more-link {
  font-size: 16px;
}

/* Button Styles */
button,
input[type='button'],
input[type='reset'],
input[type='submit'],
.btn {
  width: auto;
  padding: 16px 50px;
  border: none;
  border-bottom: 0;
  border-radius: 0;
  color: #fff;
  background: #0793ca;
  font-family: 'Open Sans', -apple-system, BlinkMacSystemFont, 'Segoe UI',
    Roboto, Helvetica, Arial, sans-serif, 'Apple Color Emoji', 'Segoe UI Emoji',
    'Segoe UI Symbol';
  font-size: 16px;
  line-height: 1.3;
  letter-spacing: 1px;
  font-weight: 700;
}
/* Homepage CTA Button */
a.btn.themotion-scroll-to-section {
  width: auto;
  padding: 16px 50px;
  border: none;
  border-bottom: 0;
  border-radius: 0;
  color: #fff;
  background: #38a865;
  font-family: 'Open Sans', -apple-system, BlinkMacSystemFont, 'Segoe UI',
    Roboto, Helvetica, Arial, sans-serif, 'Apple Color Emoji', 'Segoe UI Emoji',
    'Segoe UI Symbol';
  font-size: 16px;
  line-height: 1.3;
  letter-spacing: 1px;
  font-weight: 700;
}

/* Styling for single post date */
.post-date {
  color: #aaa;
  font-family: 'Open Sans', Helvetica, sans-serif;
  letter-spacing: 2px;
  font-weight: 700;
}

h1.entry-title,
h1 {
  font-size: 40px;
  line-height: 1.2;
  letter-spacing: 1.25px;
  color: #333;
}
h2.entry-content,
h2 {
  font-size: 34px;
  line-height: 1.3;
  letter-spacing: 1px;
  color: #333;
  font-weight: 800;
}

h3.entry-content,
h3 {
  font-size: 28px;
  line-height: 1.3;
  font-weight: 700;
  color: #333;
  letter-spacing: 1px;
}

h4.entry-content,
h4 {
  font-size: 24px;
  line-height: 1.3;
  font-weight: 700;
  color: #333;
  letter-spacing: 1px;
}

h5.entry-content,
h5 {
  font-size: 20px;
  line-height: 1.3;
  font-weight: 700;
  color: #333;
  letter-spacing: 0.75px;
}

h6.entry-content,
h6 {
  font-size: 18px;
  line-height: 1.3;
  font-weight: 700;
  color: #333;
  letter-spacing: 0px;
}

/* Editor Style Override */
#tinymce {
  line-height: 34px !important;
}

@media screen and (min-width: 480px) .free-trial-cta-container img {
  max-width: 50%;
}

.wp-block-embed-instagram {
	margin: 0 !important;
}
